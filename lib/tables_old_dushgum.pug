body
    section#container
        //
          **********************************************************************************************************************************************************
          TOP BAR CONTENT & NOTIFICATIONS
          ***********************************************************************************************************************************************************
        // header start
        header.header.black-bg
            .sidebar-toggle-box
                .fa.fa-bars.tooltips(data-placement='right', data-original-title='Toggle Navigation')
            // logo start
            a.logo(href='index.html')
                b OSA TEST SUITE REPORT
            // logo end
            #top_menu.nav.notify-row
                // notification start
                // notification end

            .top-menu
        // header end
        //
          **********************************************************************************************************************************************************
          MAIN SIDEBAR MENU
          ***********************************************************************************************************************************************************
        // sidebar start
        aside
            #sidebar.nav-collapse
                // sidebar menu start
                ul#nav-accordion.sidebar-menu
                    p.centered
                        a(href='profile.html')
                            img.img-circle(src='../template/img/ui-sam.jpg', width='60')

                    h5.centered

                    li.mt
                        a(href='index.html')
                            i.fa.fa-dashboard

                            span Dashboard

                    li.sub-menu
                        a.active(href='tables.html')
                            i.fa.fa-th

                            span Data Tables

                // sidebar menu end
        // sidebar end
        //
          **********************************************************************************************************************************************************
          MAIN CONTENT
          ***********************************************************************************************************************************************************
        // main content start

        section#main-content
            section.wrapper
                    .row.mt
                        .col-md-9
                            .content-panel
                                table.table.table-striped.table-advance.table-hover.table-condensed(style='border-collapse:collapse;')
                                    thead
                                        tr
                                            th #
                                             
                                            th Test name
                                             
                                            th Status
                                             
                                            th Time
                                             
                                    tbody
                                        - var tests=nameTest
                                            each name in tests
                                                tr.test(data-toggle='collapse', data-target='.demo')
                                                    td 1
                                                    td=name
                                                    td
                                                        button.btn.btn-success.btn-xs
                                                            i.fa.fa-check Passed
                                                    td time

                                                tr
                                                    td.hiddenRow
                                                        .collapse.demo 2
                                                    td.hiddenRow
                                                        .collapse.demo test
                                                    td.hiddenRow
                                                        .collapse.demo
                                                        button.btn.btn-danger.btn-xs
                                                            i.fa.fa-bug Failed
                                                    td.hiddenRow
                                                        .collapse.demo time


                            // /content-panel
                        // /col-md-12
                        //
                           /row

